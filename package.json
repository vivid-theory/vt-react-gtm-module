{
  "name": "@vividtheory/vt-react-gtm-module",
  "version": "0.1.3",
  "description": "React Google Tag Manager Module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn run build:lib",
    "build:watch": "yarn run build:lib -watch",
    "build-storybook": "build-storybook",
    "build:lib": "tsc",
    "build:lib:dev": "tsc --incremental",
    "check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*{.ts,.tsx}\"",
    "gen": "npx generate-react-cli",
    "lint": "eslint --fix --ext .ts,.tsx ./src",
    "pkg-ok": "pkg-ok",
    "prepublish": "yarn run build",
    "storybook": "start-storybook -p 3010",
    "test": "yarn run check && yarn run pkg-ok",
    "test:lint": "eslint '{src}/**/*.ts'",
    "test:workflow": "echo PASSED. Test not integrated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivid-theory/vt-react-gtm-module.git"
  },
  "author": "Allen Lu <allen@vividtheory.com> (https://github.com/apluualberta)",
  "homepage": "https://github.com/vivid-theory/vt-react-gtm-module#readme",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "google tag manager",
    "tag manager",
    "gtm"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "6.18.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.18.2",
    "jest": "^19.0.2",
    "jest-cli": "^21.2.1",
    "sinon": "^1.14.1"
  },
  "jest": {
    "testRegex": "__tests__.*\\.spec.js$",
    "collectCoverage": true
  },
  "dependencies": {
    "@types/jquery": "^3.5.14",
    "awesome-typescript-loader": "^5.2.1",
    "source-map-loader": "^4.0.1"
  },
  "bugs": {
    "url": "https://github.com/vivid-theory/vt-react-gtm-module/issues"
  },
  "license": "ISC"
}
